<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MemberTab">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="357dp"
        android:layout_height="182dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="520dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/so" />

    <Button
        android:id="@+id/b_notice"
        android:layout_width="135dp"
        android:layout_height="100dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="101dp"
        android:layout_marginEnd="264dp"
        android:layout_marginBottom="371dp"
        android:text="Notice"
        android:background="@drawable/btn_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/b_comp"
        android:layout_width="135dp"
        android:layout_height="100dp"
        android:layout_marginStart="94dp"
        android:layout_marginTop="101dp"
        android:layout_marginEnd="74dp"
        android:layout_marginBottom="373dp"
        android:text="Complaints"
        android:background="@drawable/btn_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/b_notice"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/b_staff"
        android:layout_width="135dp"
        android:layout_height="100dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="259dp"
        android:layout_marginBottom="227dp"
        android:text="Staff"
        android:background="@drawable/btn_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_notice" />

    <Button
        android:id="@+id/b_rules"
        android:layout_width="135dp"
        android:layout_height="100dp"
        android:layout_marginStart="98dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="74dp"
        android:layout_marginBottom="227dp"
        android:text="Rules"
        android:background="@drawable/btn_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/b_staff"
        app:layout_constraintTop_toBottomOf="@+id/b_comp" />

    <Button
        android:id="@+id/b_album"
        android:layout_width="135dp"
        android:layout_height="100dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="254dp"
        android:layout_marginBottom="83dp"
        android:text="Society Album"
        android:background="@drawable/btn_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_staff" />

    <Button
        android:id="@+id/b_main"
        android:layout_width="135dp"
        android:layout_height="100dp"
        android:layout_marginStart="98dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="74dp"
        android:layout_marginBottom="83dp"
        android:text="Maintenance"
        android:background="@drawable/btn_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/b_album"
        app:layout_constraintTop_toBottomOf="@+id/b_rules" />
</androidx.constraintlayout.widget.ConstraintLayout>